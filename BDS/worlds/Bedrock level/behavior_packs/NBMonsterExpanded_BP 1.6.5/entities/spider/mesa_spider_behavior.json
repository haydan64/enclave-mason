						},
						"cause": "lava",
						"damage_per_tick": 4
					}
				]
			},
			"minecraft:movement": {
				"value": 0.3
			},
			
			"minecraft:navigation.climb": {
				"can_path_over_water": false,
				"avoid_water": true
			},
			"minecraft:movement.basic": {},
			"minecraft:can_climb": {},
