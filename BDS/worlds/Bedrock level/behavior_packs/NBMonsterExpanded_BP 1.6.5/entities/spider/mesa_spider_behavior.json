					}
					
				]
			},
			"minecraft:become_hostile": {
				"remove": {
					"component_groups": ["minecraft:spider_neutral"]
				},
				"add": {
					"component_groups": ["minecraft:spider_hostile"]
				}
			},
			"minecraft:become_neutral": {
				"remove": {
					"component_groups": ["minecraft:spider_hostile"]
