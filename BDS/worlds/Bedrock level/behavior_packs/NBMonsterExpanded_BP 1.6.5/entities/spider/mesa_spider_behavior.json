				  "looping": false,
				  "time": 0.5,
				  "time_down_event": {
					"event": "notstunned"
				  }
				}
			}
		},
		"components": {
			"minecraft:behavior.delayed_attack": {
				"priority": 3,
				"track_target": true,
				"attack_duration": 0.83,
				"speed_multiplier": 1,
				"reach_multiplier": 2
			},
			"minecraft:damage_sensor": {
				"triggers": [
				  {
					"on_damage": {
					  "filters": {
						"all_of": [
						  { "test": "has_damage", "value": "attack" },
						  { "test": "has_component", "subject": "self", "operator": "!=", "value": "minecraft:is_stunned" }
						]
					  },
					  "event": "stunned"
					},
					"deals_damage": true
				  }
				]
			  },
			  "minecraft:on_death": {
				"event": "corpse1",
				"target": "self"
			},
			"minecraft:is_hidden_when_invisible": {
			},
