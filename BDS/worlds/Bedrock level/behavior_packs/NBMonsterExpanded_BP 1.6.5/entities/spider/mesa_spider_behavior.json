				"suffocate_time": 0
			},
			"minecraft:loot": {
				"table": "loot_tables/entities/spider.json"
			},
			"minecraft:collision_box": {
				"width": 1,
				"height": 0.5
			},
			"minecraft:custom_hit_test": {
				"hitboxes": [
					{
						"width": 1,
						"height": 0.5,
						"pivot": [0, 0.55, 0]
					}
				]
			},
			"minecraft:health": {
				"value": 16,
				"max": 16
			},
			"minecraft:hurt_on_condition": {
