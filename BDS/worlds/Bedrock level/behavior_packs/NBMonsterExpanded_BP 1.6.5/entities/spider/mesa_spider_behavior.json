							{
								"weight": 80,
								"filters": {
									"all_of": [
										{
											"test": "is_daytime",
											"value": false
										},
										{
											"test": "is_snow_covered",
											"value": true
										},
										{
											"test": "is_underground",
											"value": false
										}
